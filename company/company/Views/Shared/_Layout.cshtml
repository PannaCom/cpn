    <!-- Bootstrap tooltips -->
    <script type="text/javascript" src="/Content/MDBFree/js/tether.min.js"></script>
    <!-- Bootstrap core JavaScript -->
    <script type="text/javascript" src="/Content/MDBFree/js/bootstrap.min.js"></script>
    <!-- MDB core JavaScript -->
